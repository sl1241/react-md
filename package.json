{
  "name": "react-md-main",
  "private": true,
  "version": "2.4.0",
  "description": "The mono-repo for react-md",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "release//": "git add . && git commit -m \"Bump version\" git push && pnpm version patch",
    "prepare//": "husky install",
    "rmd-codemod": "node packages/codemod/bin/rmd-codemod.js",
    "release": "dev-utils release",
    "sandbox": "dev-utils sandbox",
    "sassdoc": "dev-utils sassdoc",
    "start": "pnpm workspace documentation start-dev",
    "dev": "npm-run-all -p watch start",
    "setup": "npm-run-all build-dev-utils force-install build \"sandbox --empty\"",
    "force-install": "pnpm install --force",
    "combine-styles": "dev-utils combine-styles",
    "styles": "dev-utils styles",
    "variables": "dev-utils variables",
    "run-indexer": "dev-utils doc-index",
    "generate-typedoc": "dev-utils typedoc",
    "build-docs-meta": "npm-run-all sandbox sassdoc run-indexer generate-typedoc",
    "build-docs": "pnpm workspace documentation build",
    "build-website": "npm-run-all build-docs-meta build-docs",
    "build-dev-utils": "pnpm workspace @react-md/dev-utils build",
    "build-ejs": "tsc -b tsconfig.ejs.json",
    "build-cjs": "tsc -b tsconfig.cjs.json",
    "build-var": "tsc -b tsconfig.var.json",
    "build-umd": "pnpm workspace react-md umd --silent",
    "build-codemod": "pnpm workspace @react-md/codemod build",
    "build": "npm-run-all styles combine-styles build-ejs build-cjs build-var build-codemod",
    "typecheck": "tsc -p tsconfig.check.json",
    "//lint-scripts": "eslint \"packages/*/src/**/*.{ts,tsx,js,jsx}\"",
    "//lint-styles": "stylelint \"packages/*/src/**/*.scss\"",
    "//lint": "npm-run-all lint-scripts lint-styles typecheck",
    "//test": "node --experimental-modules ./node_modules/jest/bin/jest.js",
    "//format": "prettier --write \"**/*.{ts,tsx,scss,js,jsx,md,yml,json}\"",
    "clean": "dev-utils clean",
    "clean-dev-utils": "pnpm workspace @react-md/dev-utils clean",
    "clean-all": "npm-run-all clean clean-dev-utils",
    "watch11": "dev-utils watch",
    "watch:react": "cd ./packages/react && vite build --watch",
    "watch:utils": "cd ./packages/utils && vite build --watch",
    "watch:theme": "cd ./packages/theme && vite build --watch",
    "watch:transition": "cd ./packages/transition && vite build --watch",
    "watch:states": "cd ./packages/states && vite build --watch",
    "watch:typography": "cd ./packages/typography && vite build --watch",
    "watch:icon": "cd ./packages/icon && vite build --watch",
    "watch:button": "cd ./packages/button && vite build --watch",
    "watch:alert": "cd ./packages/alert && vite build --watch",
    "watch:appbar": "cd ./packages/app-bar && vite build --watch",
    "watch:elevation": "cd ./packages/elevation && vite build --watch && tsc --declaration --watch",
    "watch:avatar": "cd ./packages/avatar && vite build --watch",
    "watch:divider": "cd ./packages/divider && vite build --watch",
    "watch:progress": "cd ./packages/progress && vite build --watch",
    "watch:portal": "cd ./packages/portal && vite build --watch",
    "watch:overlay": "cd ./packages/overlay && vite build --watch",
    "watch:dialog": "cd ./packages/dialog && vite build --watch",
    "watch:list": "cd ./packages/list && vite build --watch",
    "watch:sheet": "cd ./packages/sheet && vite build --watch",
    "watch:menu": "cd ./packages/menu && vite build --watch",
    "watch:form": "cd ./packages/form && vite build --watch",
    "watch:autocomplete": "cd ./packages/autocomplete && vite build --watch",
    "watch:all": "run-s watch:react watch:utils watch:states watch:typography watch:icon watch:button watch:alert watch:appbar watch:elevation watch:avatar watch:divider watch:list watch:progress watch:portal watch:overlay watch:dialog watch:sheet watch:menu",
    "build:react": "cd ./packages/react && pnpm build",
    "build:utils": "cd ./packages/utils && pnpm build",
    "build:theme": "cd ./packages/theme && pnpm build",
    "build:portal": "cd ./packages/portal && pnpm build",
    "build:transition": "cd ./packages/transition && pnpm build",
    "build:states": "cd ./packages/states && pnpm build",
    "build:typography": "cd ./packages/typography && pnpm build",
    "build:icon": "cd ./packages/icon && pnpm build",
    "build:elevation": "cd ./packages/elevation && pnpm build",
    "build:button": "cd ./packages/button && pnpm build",
    "build:alert": "cd ./packages/alert && pnpm build",
    "build:appbar": "cd ./packages/app-bar && pnpm build",
    "build:avatar": "cd ./packages/avatar && pnpm build",
    "build:divider": "cd ./packages/divider && pnpm build",
    "build:progress": "cd ./packages/progress && pnpm build",
    "build:overlay": "cd ./packages/overlay && pnpm build",
    "build:dialog": "cd ./packages/dialog && pnpm build",
    "build:list": "cd ./packages/list && pnpm build",
    "build:sheet": "cd ./packages/sheet && pnpm build",
    "build:menu": "cd ./packages/menu && pnpm build",
    "build:tooltip": "cd ./packages/tooltip && pnpm build",
    "build:form": "cd ./packages/form && pnpm build",
    "build:autocomplete": "cd ./packages/autocomplete && pnpm build",
    "build:badge": "cd ./packages/badge && pnpm build",
    "build:card": "cd ./packages/card && pnpm build",
    "build:chip": "cd ./packages/chip && pnpm build",
    "build:expansion-panel": "cd ./packages/expansion-panel && pnpm build",
    "build:tree": "cd ./packages/tree && pnpm build",
    "build:link": "cd ./packages/link && pnpm build",
    "build:layout": "cd ./packages/layout && pnpm build",
    "build:material-icons": "cd ./packages/material-icons && pnpm compile",
    "build:media": "cd ./packages/media && pnpm build",
    "build:table": "cd ./packages/table && pnpm build",
    "build:tabs": "cd ./packages/tabs && pnpm build",
    "build:all": "run-s build:react build:utils watch:theme build:states build:typography build:icon build:button build:alert build:appbar build:elevation build:avatar build:divider build:list build:progress build:portal build:overlay build:dialog build:sheet build:menu build:form build:autocomplete build:badge build:card build:chip build:expansion-panel build:tree build:link build:layout build:material-icons build:media build:table build:tabs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaursen/react-md.git"
  },
  "author": "Mikkel Laursen <mlaursen03@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mlaursen/react-md/issues"
  },
  "homepage": "https://react-md.dev",
  "dependencies": {
    "pnpm": "^8.3.1",
    "voby": "workspace:../woby"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@mlaursen/eslint-config": "^1.9.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@types/prettier": "^2.7.2",
    "@types/reach__router": "^1.3.11",
    "@types/react-dom": "^17.0.20",
    "@types/react-test-renderer": "^17.0.2",
    "chokidar": "^3.5.3",
    "dev-utils": "workspace:./packages/dev-utils",
    "eslint": "^8.39.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jasmine-browser-runner": "^2.0.0",
    "jasmine-core": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^1.1.0",
    "lerna": "^4.0.0",
    "lint-staged": "^12.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react-test-renderer": "^17.0.2",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.6.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "//lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint -c .precommit.eslintrc.js --fix",
      "prettier --write"
    ],
    "**/*.{scss,js,jsx,md,yml,json}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}