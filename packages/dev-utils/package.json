{
  "name": "@react-md/dev-utils",
  "version": "5.1.3",
  "description": "Common build utilities for react-md. Contains helpers for building, watching, testing, etc",
  "private": true,
  "bin": {
    "dev-utils": "./bin/cli.js"
  },
  "scripts": {
    "clean": "rimraf bin dist",
    "executable": "node executable.js",
    "compile": "tsc",
    "build": "npm-run-all compile executable",
    "watcher": "chokidar \"bin/cli.js\" -c \"npm run executable\"",
    "watch": "npm-run-all -p \"compile -w\" watcher"
  },
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@octokit/core": "^3.6.0",
    "chokidar": "^3.5.3",
    "commander": "^9.5.0",
    "cssnano": "^5.1.15",
    "dotenv": "^16.0.3",
    "filesize": "^8.0.7",
    "fs-extra": "^10.1.0",
    "glob": "^8.1.0",
    "gzip-size": "^6.0.0",
    "he": "^1.2.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1",
    "markdown-toc": "^1.2.0",
    "marked": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.7",
    "pretty-ms": "^7.0.1",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "sass": "^1.60.0",
    "sassdoc": "^2.7.4",
    "ts-morph": "^14.0.0",
    "typedoc": "^0.22.18",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/he": "^1.2.0",
    "@types/marked": "^4.0.8",
    "@types/prompts": "^2.4.3",
    "@types/react": "^18.0.29",
    "@types/sass": "^1.45.0",
    "chokidar-cli": "^3.0.0",
    "csstype": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
